{
  "prettier.singleQuote": true,
  "files.autoSave": "afterDelay",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.tabSize": 2,
  "workbench.iconTheme": "eq-material-theme-icons",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.eol": "\n",
  "typescript.preferences.importModuleSpecifier": "relative",
  "eslint.packageManager": "yarn",
  "npm.packageManager": "yarn",
  "diffEditor.wordWrap": "on",
  "editor.wordWrap": "on",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "redhat.telemetry.enabled": false,
  "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m -javaagent:\"c:\\Users\\Anson\\.vscode\\extensions\\gabrielbb.vscode-lombok-1.0.1\\server\\lombok.jar\"",
  "[nginx]": {
    "editor.defaultFormatter": "raynigon.nginx-formatter"
  },
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "workbench.startupEditor": "none",
  "explorer.compactFolders": false,
  "npm.keybindingsChangedWarningShown": true,
  "editor.cursorWidth": 25,
  "http.proxyAuthorization": null,
  "workbench.colorTheme": "Field Lights",
  "github.gitProtocol": "ssh",
  "git.showPushSuccessNotification": true,
  "editor.minimap.autohide": true,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "editor.stickyScroll.enabled": true,
  "editor.cursorBlinking": "smooth",
  "omnisharp.useModernNet": false,
  "window.zoomLevel": 1,
  "material-icon-theme.activeIconPack": "nest",
  "workbench.list.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "terminal.integrated.smoothScrolling": true,
  "editor.fontSize": 20,
  "editor.cursorSmoothCaretAnimation": true
}
